    "license": "MIT",
    "bugs": {
        "url": "https://github.com/misantronic/select/issues"
    },
    "homepage": "https://github.com/misantronic/select#readme",
    "peerDependencies": {
        "react": ">= 16.0.0",
        "react-dom": ">= 16.0.0"
    },
    "dependencies": {
        "lodash-decorators": ">= 6.0.0",
        "react-virtualized": "^9.21.0",
        "styled-components": ">= 4.x"
    },
    "devDependencies": {
        "@types/react": "^16.7.x",
        "@types/react-dom": "^16.0.x",
        "@types/react-virtualized": "^9.18.x",
        "@types/styled-components": "^4.1.x",
        "fuse-box": "3.7.0-next.6",
        "object-assign": "^4.1.1",
        "parcel-bundler": "^1.9.3",
        "terser": "^3.13.1",
